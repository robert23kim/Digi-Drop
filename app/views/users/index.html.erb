<h2>All Users</h2>

<%#  Part 2: Start here... %>

<table class="table table-striped col-md-12" id="users">
  <thead>
    <tr>
      <th>Users</th>
      <th>Colletibles/th>
      <th>Value</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td>
          <%= link_to '#{user.username}', user_path(user) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
